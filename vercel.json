{
    "version": 2,
    "builds": [
      {
        "src": "ai_aggregator/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": {
              "distDir": "staticfiles_build"
          }
      }
    ],
    "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "ai_aggregator/wsgi.py"
      }
    ]
  }